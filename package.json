{
  "name": "qoopido.demand",
  "title": "Qoopido.demand",
  "description": "Promise like module loader using XHR requests and localStorage caching to dynamically load JavaScript and CSS + dynamic dependency resolution + support for custom handlers",
  "version": "3.5.2",
  "homepage": "https://github.com/dlueth/qoopido.demand",
  "author": {
    "name": "Dirk Lueth",
    "email": "info@qoopido.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dlueth/qoopido.demand.git"
  },
  "bugs": {
    "url": "https://github.com/dlueth/qoopido.demand/issues"
  },
  "license": "MIT or GPL-3.0+",
  "main": "dist/demand.js",
  "filename": "demand.js",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/dlueth/qoopido.demand.git",
    "basePath": "/dist",
    "files": [
      "**/*"
    ]
  },
  "keywords": [
    "module",
    "modular",
    "script",
    "loader",
    "javascript",
    "css",
    "promise",
    "xhr",
    "ajax",
    "localStorage",
    "cache",
    "caching",
    "asset",
    "storage",
    "require",
    "basket"
  ],
  "devDependencies": {
    "del": "^2.0.2",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.2.0",
    "gulp-chmod": "^1.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-declare": "^0.3.0",
    "gulp-define-module": "^0.1.4",
    "gulp-devtools": "0.0.3",
    "gulp-eslint": "^3.0.1",
    "gulp-frep": "^0.1.3",
    "gulp-header": "^1.7.1",
    "gulp-include": "^2.3.1",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.6",
    "run-sequence": "^1.1.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "no-cond-assign": 0,
      "camelcase": [
        "error"
      ],
      "no-restricted-syntax": [
        "error",
        "ForInStatement",
        "DebuggerStatement",
        "LabeledStatement",
        "WithStatement"
      ]
    },
    "env": {
      "browser": true
    },
    "globals": {
      "demand": true,
      "provide": true
    }
  }
}
